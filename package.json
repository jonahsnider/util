{
	"name": "@jonahsnider/util",
	"version": "0.0.0-development",
	"description": "A useful collection of optimized utility functions",
	"homepage": "https://util.jonah.pw/",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonahsnider/util.git"
	},
	"license": "MIT",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"sideEffects": false,
	"type": "commonjs",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.prod.mjs"
		},
		"./prod": {
			"require": "./dist/index.prod.js",
			"import": "./dist/index.prod.mjs"
		},
		"./dev": {
			"require": "./dist/index.dev.js",
			"import": "./dist/index.dev.mjs"
		}
	},
	"main": "./dist/index.js",
	"jsdelivr": "./dist/index.prod.mjs",
	"unpkg": "./dist/index.prod.mjs",
	"module": "./dist/index.prod.mjs",
	"browser": "./dist/index.prod.mjs",
	"typings": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/tsc_types"
	],
	"scripts": {
		"analyze": "size-limit --why",
		"build": "tsc && rollup --config rollup.config.js",
		"deploy": "semantic-release",
		"docs": "typedoc --options ./typedoc.json",
		"lint": "xo",
		"size": "size-limit",
		"style": "prettier --check .",
		"test": "jest",
		"validate-api": "api-extractor run",
		"validate-api:local": "api-extractor run --local --verbose"
	},
	"prettier": "@jonahsnider/prettier-config",
	"resolutions": {
		"terser": "5.10.0"
	},
	"devDependencies": {
		"@babel/core": "7.16.5",
		"@babel/preset-env": "7.16.5",
		"@jonahsnider/prettier-config": "1.1.0",
		"@jonahsnider/xo-config": "5.0.3",
		"@microsoft/api-extractor": "7.19.2",
		"@rollup/plugin-babel": "5.3.0",
		"@rollup/plugin-replace": "3.0.0",
		"@rollup/plugin-typescript": "8.3.0",
		"@size-limit/preset-small-lib": "7.0.4",
		"@tsconfig/node16": "1.0.2",
		"@types/jest": "27.0.3",
		"@types/node": "16.11.15",
		"@types/xo": "0.39.2",
		"jest": "27.4.5",
		"prettier": "2.5.1",
		"rollup": "2.61.1",
		"rollup-plugin-terser": "7.0.2",
		"semantic-release": "18.0.1",
		"size-limit": "7.0.4",
		"terser": "5.10.0",
		"ts-jest": "27.1.2",
		"tsd": "0.19.0",
		"typedoc": "0.22.10",
		"typescript": "4.5.4",
		"xo": "0.47.0"
	},
	"engines": {
		"node": ">=16"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.1.1",
	"size-limit": [
		{
			"brotli": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.prod.js"
		},
		{
			"gzip": true,
			"limit": "6.5 KiB",
			"path": "./dist/index.prod.js"
		},
		{
			"brotli": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.prod.mjs"
		},
		{
			"gzip": true,
			"limit": "6.5 KiB",
			"path": "./dist/index.prod.mjs"
		}
	]
}
