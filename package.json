{
	"name": "@jonahsnider/util",
	"version": "0.0.0-development",
	"description": "A useful collection of optimized utility functions",
	"homepage": "https://util.jonah.pw/",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonahsnider/util.git"
	},
	"license": "MIT",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"sideEffects": false,
	"type": "commonjs",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.js",
	"jsdelivr": "./dist/index.mjs",
	"unpkg": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"browser": "./dist/index.mjs",
	"typings": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/tsc_types"
	],
	"scripts": {
		"analyze": "size-limit --why",
		"build": "tsc && rollup --config rollup.config.js --bundleConfigAsCjs",
		"deploy": "semantic-release",
		"docs": "typedoc --options ./typedoc.json",
		"lint": "xo",
		"size": "size-limit",
		"style": "prettier --check .",
		"test": "vitest --run --no-isolate",
		"test:coverage": "vitest --run --no-isolate --coverage",
		"validate-api": "api-extractor run",
		"validate-api:local": "api-extractor run --local --verbose"
	},
	"prettier": "@jonahsnider/prettier-config",
	"devDependencies": {
		"@babel/core": "7.22.20",
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.3.0",
		"@microsoft/api-extractor": "7.37.0",
		"@rollup/plugin-babel": "6.0.3",
		"@rollup/plugin-typescript": "11.1.3",
		"@size-limit/preset-small-lib": "9.0.0",
		"@tsconfig/node16": "16.1.1",
		"@types/node": "18.17.18",
		"@vitest/coverage-v8": "0.34.5",
		"@vitest/ui": "0.34.5",
		"prettier": "3.0.3",
		"rollup": "3.29.2",
		"rollup-plugin-terser": "7.0.2",
		"semantic-release": "22.0.1",
		"size-limit": "9.0.0",
		"tsd": "0.29.0",
		"typedoc": "0.25.1",
		"typescript": "5.2.2",
		"vitest": "0.34.5",
		"xo": "0.56.0"
	},
	"engines": {
		"node": ">=16"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.6.3",
	"size-limit": [
		{
			"brotli": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.js"
		},
		{
			"gzip": true,
			"limit": "6.5 KiB",
			"path": "./dist/index.js"
		},
		{
			"brotli": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.mjs"
		},
		{
			"gzip": true,
			"limit": "6.5 KiB",
			"path": "./dist/index.mjs"
		}
	]
}
