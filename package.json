{
	"name": "@jonahsnider/util",
	"version": "0.0.0-development",
	"description": "A useful collection of optimized utility functions",
	"homepage": "https://util.jonah.pw/",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonahsnider/util.git"
	},
	"license": "MIT",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"sideEffects": false,
	"type": "commonjs",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.prod.mjs"
		},
		"./prod": {
			"require": "./dist/index.prod.js",
			"import": "./dist/index.prod.mjs"
		},
		"./dev": {
			"require": "./dist/index.dev.js",
			"import": "./dist/index.dev.mjs"
		}
	},
	"main": "./dist/index.js",
	"jsdelivr": "./dist/index.prod.mjs",
	"unpkg": "./dist/index.prod.mjs",
	"module": "./dist/index.prod.mjs",
	"browser": "./dist/index.prod.mjs",
	"typings": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"analyze": "size-limit --why",
		"docs": "typedoc --options ./typedoc.json",
		"publish": "semantic-release",
		"build": "rollup --config rollup.config.js",
		"size": "size-limit",
		"test": "jest",
		"style": "prettier --check ."
	},
	"resolutions": {
		"terser": "5.9.0"
	},
	"devDependencies": {
		"@babel/core": "7.15.5",
		"@babel/preset-env": "7.15.6",
		"@rollup/plugin-babel": "5.3.0",
		"@rollup/plugin-replace": "3.0.0",
		"@rollup/plugin-typescript": "8.2.5",
		"@size-limit/preset-small-lib": "5.0.4",
		"@tsconfig/node16": "1.0.2",
		"@types/jest": "27.0.2",
		"jest": "27.2.1",
		"prettier": "2.4.1",
		"prettier-config-xo": "2.0.0",
		"rollup": "2.57.0",
		"rollup-plugin-terser": "7.0.2",
		"semantic-release": "18.0.0",
		"size-limit": "5.0.4",
		"terser": "5.9.0",
		"ts-jest": "27.0.5",
		"tsd": "0.17.0",
		"typedoc": "0.22.4",
		"typescript": "4.4.3"
	},
	"engines": {
		"node": ">=16"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.0.2",
	"size-limit": [
		{
			"brotli": true,
			"limit": "4.5 KiB",
			"path": "./dist/index.prod.js",
			"webpack": false
		},
		{
			"gzip": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.prod.js",
			"webpack": false
		},
		{
			"brotli": true,
			"limit": "4.5 KiB",
			"path": "./dist/index.prod.mjs",
			"webpack": false
		},
		{
			"gzip": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.prod.mjs",
			"webpack": false
		}
	]
}
