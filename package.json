{
	"name": "@jonahsnider/util",
	"version": "0.0.0-development",
	"description": "A useful collection of optimized utility functions",
	"homepage": "https://util.jonah.pw/",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonahsnider/util.git"
	},
	"license": "MIT",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"sideEffects": false,
	"type": "commonjs",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.js",
	"jsdelivr": "./dist/index.mjs",
	"unpkg": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"browser": "./dist/index.mjs",
	"typings": "./dist/index.d.ts",
	"files": [
		"dist",
		"!dist/tsc_types"
	],
	"scripts": {
		"analyze": "size-limit --why",
		"build": "tsc && rollup --config rollup.config.js --bundleConfigAsCjs",
		"deploy": "semantic-release",
		"docs": "typedoc --options ./typedoc.json",
		"lint": "xo",
		"size": "size-limit",
		"style": "prettier --check .",
		"test": "vitest --run --no-isolate",
		"test:coverage": "vitest --run --no-isolate --coverage",
		"validate-api": "api-extractor run",
		"validate-api:local": "api-extractor run --local --verbose"
	},
	"prettier": "@jonahsnider/prettier-config",
	"resolutions": {
		"terser": "5.16.6"
	},
	"devDependencies": {
		"@babel/core": "7.21.3",
		"@babel/preset-env": "7.20.2",
		"@jonahsnider/prettier-config": "1.1.2",
		"@jonahsnider/xo-config": "5.3.0",
		"@microsoft/api-extractor": "7.34.4",
		"@rollup/plugin-babel": "6.0.3",
		"@rollup/plugin-typescript": "11.0.0",
		"@size-limit/preset-small-lib": "8.2.4",
		"@tsconfig/node16": "1.0.3",
		"@types/node": "18.15.8",
		"@vitest/coverage-c8": "0.29.7",
		"@vitest/ui": "0.29.7",
		"prettier": "2.8.7",
		"rollup": "3.20.1",
		"rollup-plugin-terser": "7.0.2",
		"semantic-release": "20.1.3",
		"size-limit": "8.2.4",
		"terser": "5.16.6",
		"tsd": "0.28.0",
		"typedoc": "0.23.28",
		"typescript": "4.9.5",
		"vite": "4.2.1",
		"vitest": "0.29.7",
		"xo": "0.53.1"
	},
	"engines": {
		"node": ">=16"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.5.0",
	"size-limit": [
		{
			"brotli": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.js"
		},
		{
			"gzip": true,
			"limit": "6.5 KiB",
			"path": "./dist/index.js"
		},
		{
			"brotli": true,
			"limit": "5.5 KiB",
			"path": "./dist/index.mjs"
		},
		{
			"gzip": true,
			"limit": "6.5 KiB",
			"path": "./dist/index.mjs"
		}
	]
}
